(module jazz.ui.skin jazz
  (import (jazz.component))
  (import (jazz.graphic))
  (import (jazz.jml))
  (import (jazz.skin))
  (import (jazz.ui))
  (class UI-Skin extends Skin
    (method meta protected override (set-class-form form))
    (method meta protected override (get-class-form))
    (method meta protected override (get-class-forms)))
  (class Border-Skin extends Skin
    (property edge-flat? accessors generate))
