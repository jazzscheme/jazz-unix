    (property unfocused-text-color accessors generate))
  (class Tree-Skin extends Skin
    (property background accessors generate)
    (property text-color accessors generate)
    (property line-color accessors generate)
    (property focused-border accessors generate)
    (property focused-color accessors generate)
    (property unfocused-border accessors generate)
    (property unfocused-color accessors generate)))
