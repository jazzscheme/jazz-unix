    (property background accessors generate)
    (property title-color accessors generate)
    (property flat? accessors generate))
  (class Button-Skin extends Skin
    (property background accessors generate))
  (class Check-Skin extends Skin
    (property text-color accessors generate))
  (class Radio-Skin extends Skin
    (property text-color accessors generate))
  (class Combo-Skin extends Skin
    (property left-padding accessors generate)
    (property top-padding accessors generate)
    (property text-font accessors generate)
    (property arrow-color accessors generate)
    (property button-border accessors generate))
